program main

    declare a, b, c;

    function f1(in a, inout b)

        {
            b:=a+1;
            c:=4;
            if(a=1){
                c:=1;
            }else{
                c:= c + 1;
            };
            return(b);
        }
        
    {
        a:=1;
        c:=f1(in a, inout b);
        print(c);
        print(b);
    }.
        
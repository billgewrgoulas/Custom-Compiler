program calculator
	declare choice, x, y;
	
	function add(in x, in y)
	{
		return (x + y);
	}
	function sub(in x, in y)
	{
		return (x - y);
	}
	function mul(in x, in y)
	{
		return (x * y);
	}
	function divide(in x, in y)
	{
		return (x / y);
	}
	#main program!!!#
	{
		input(choice);
		input(x);
		input(y);
		if (choice = 1)
		{
			print(add(in x, in y));
			print(sub(in x, in y));
		}
		else
		{
			print(mul(in x, in y));
			print(divide(in x, in y));
		}
	}.